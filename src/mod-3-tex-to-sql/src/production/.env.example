# Production Environment Variables Template
# Copy this file to .env and fill in your actual values

# OpenAI Configuration
OPENAI_API_KEY=sk-proj-your-openai-api-key-here
OPENAI_MODEL=gpt-3.5-turbo
EMBEDDING_MODEL=text-embedding-ada-002

# Database Configuration
DATABASE_URL=postgresql://username:password@host:port/database

# Qdrant Vector Database Configuration
QDRANT_URL=https://your-qdrant-cluster.cloud.qdrant.io:6333
QDRANT_API_KEY=your-qdrant-api-key-here

# ChromaDB Configuration
CHROMA_COLLECTION_NAME=ubereats_brasil_production
CHROMA_PATH=data/chromadb

# Logging Configuration
LOG_LEVEL=INFO
LOG_FILE=../../ubereats_brasil_vanna.log
LOG_FILE_CROSS_DB=../../ubereats_brasil_cross_db.log
LOG_FILE_MULTI_DB=../../ubereats_brasil_multi_db.log

# Application Configuration
APP_TITLE="UberEats Brasil"
APP_ICON=üçî
APP_LAYOUT=centered

# Performance Configuration
MAX_RETRIES=3
CONNECTION_TIMEOUT=30
QUERY_TIMEOUT=120

# Advanced Performance Configuration
DB_POOL_SIZE=10
DB_MAX_OVERFLOW=20
QUERY_CACHE_SIZE=500
MAX_CONCURRENT_QUERIES=10
QDRANT_POOL_SIZE=5
ENABLE_PERFORMANCE_TRACKING=true